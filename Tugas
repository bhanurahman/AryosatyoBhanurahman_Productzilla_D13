for (let i = 0; i <= 1000; i++){
    if(i % 3 === 0 && i % 5 === 0){
        console.log('angka',i,'fizzbuzz');
    } else if (i % 3 === 0) {
        console.log('angka', i, 'fizz');
    } else if (i % 5 === 0) {
        console.log('angka', i, 'buzz');
    } else {
        console.log('angka', i);
    }
}
